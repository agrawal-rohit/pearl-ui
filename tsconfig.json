{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "declaration": true,
    "strict": true,
    "noEmit": false,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules",
    "dist",
    "lib",
    "**/*.spec.ts",
    "**/*.story.[tj]sx",
    "**/*.test.[tj]sx",
    "App.test.tsx",
    "App.tsx",
    "documentationwebsite/*"
  ]
}
